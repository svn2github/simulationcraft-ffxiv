feet=boots_of_the_still_breath,id=86943,gems=80agi_160crit_60crit,enchant=140agi
finger1=painful_thorned_ring,id=86974,enchant=160agi,reforge=mastery_crit
finger2=regails_band_of_the_endless,id=90503,enchant=160agi
