#!./simc 

rogue="Rogue_Combat_T14N"
level=90
race=night_elf
spec=combat
role=attack
position=back
talents=http://us.battle.net/wow/en/tool/talent-calculator#cZ!2..0.2
glyphs=adrenaline_rush

# This default action priority list is automatically created based on your character.
# It is a attempt to provide you with a action list that is both simple and practicable,
# while resulting in a meaningful and good simulation. It may not result in the absolutely highest possible dps.
# Feel free to edit, adapt and improve it to your own needs.
# SimulationCraft is always looking for updates and improvements to the default action lists.

actions.precombat=flask,type=spring_blossoms
actions.precombat+=/food,type=sea_mist_rice_noodles
actions.precombat+=/apply_poison,lethal=deadly
actions.precombat+=/snapshot_stats
actions.precombat+=/virmens_bite_potion
actions.precombat+=/stealth

actions=virmens_bite_potion,if=buff.bloodlust.react|target.time_to_die<40
actions+=/preparation,if=talent.preparation.enabled&!buff.vanish.up&cooldown.vanish.remains>60
actions+=/auto_attack
actions+=/kick
actions+=/vanish,if=time>10&!buff.shadow_blades.up&!buff.adrenaline_rush.up&energy<20&((buff.deep_insight.up&combo_points<4)|anticipation_charges<4)
actions+=/ambush
actions+=/slice_and_dice,if=buff.slice_and_dice.remains<2|(buff.slice_and_dice.remains<15&buff.bandits_guile.stack=11&combo_points>=4)
actions+=/shadow_blades,if=(buff.bloodlust.react|time>60)&buff.slice_and_dice.remains>=buff.shadow_blades.duration
actions+=/killing_spree,if=energy<35&buff.slice_and_dice.remains>4&buff.adrenaline_rush.down
actions+=/adrenaline_rush,if=energy<35|buff.shadow_blades.up
actions+=/rupture,if=ticks_remain<2&combo_points=5&buff.deep_insight.up&target.time_to_die>10
actions+=/eviscerate,if=(combo_points=5&buff.deep_insight.up)|anticipation_charges>=4
actions+=/rupture,if=ticks_remain<2&combo_points=5&target.time_to_die>10
actions+=/revealing_strike,if=((buff.deep_insight.down&anticipation_charges<5)|(buff.deep_insight.up&combo_points<5))&ticks_remain<2
actions+=/tricks_of_the_trade
actions+=/sinister_strike,if=(buff.deep_insight.down&anticipation_charges<5)|(buff.deep_insight.up&combo_points<5)

head=helmet_of_the_thousandfold_blades,id=85301,gems=agile_primal_80agi_160hit,reforge=mastery_hit
neck=pheromonecoated_choker,id=89827
shoulders=spaulders_of_the_thousandfold_blades,id=85299,gems=80agi_160hit_60agi,enchant=200agi_100crit
back=legbreaker_greatcloak,id=86173,enchant=180crit,reforge=crit_exp
chest=tunic_of_the_thousandfold_blades,id=85303,gems=80agi_160haste_80agi_160haste_120mastery,enchant=80all,reforge=crit_haste
wrists=bracers_of_unseen_strikes,id=86163,enchant=180agi,reforge=crit_exp
hands=bonebreaker_gauntlets,id=86176,gems=80agi_160hit_60haste,enchant=170haste,reforge=hit_exp
waist=cannoneers_gunpowder_carrier,id=90451,gems=160agi_160agi_60crit,reforge=crit_haste
legs=legguards_of_the_thousandfold_blades,id=85300,gems=160agi_120agi,enchant=285agi_165crit
feet=flamefoot_tabi,id=90442,enchant=140agi
finger1=regails_band_of_the_endless,id=86231,reforge=crit_exp
finger2=hereditary_saurok_loop,id=90437,reforge=hit_crit
trinket1=bottle_of_infinite_stars,id=86132,reforge=mastery_haste
trinket2=terror_in_the_mists,id=86332
