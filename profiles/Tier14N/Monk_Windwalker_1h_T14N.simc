legs=red_crane_leggings,id=85397,gems=160agi_60agi,enchant=285agi_165crit,reforge=hit_crit
feet=boots_of_the_still_breath,id=86153,gems=80agi_160crit_60crit,enchant=140agi
finger1=regails_band_of_the_endless,id=86231
