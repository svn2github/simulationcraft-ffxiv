hands=shaskin_gloves,id=85369,enchant=170haste,reforge=haste_mastery
waist=belt_of_malleable_amber,id=86210,gems=80int_160haste_80int_160hit_160int_120haste,reforge=haste_mastery
legs=dreadwoven_leggings_of_failure,id=86389,gems=160int_80int_160hit_120int,enchant=285int_165crit,reforge=haste_mastery
feet=sandals_of_the_blackest_night,id=86330,gems=80int_160haste_60mastery,enchant=140mastery
finger1=ring_of_wretched_flames,id=90435
finger2=fragment_of_fear_made_flesh,id=86156,reforge=crit_mastery
trinket1=essence_of_terror,id=86388
