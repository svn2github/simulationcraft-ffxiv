legs=firebirds_kilt,id=85292,gems=160int_60int,enchant=285int_165spi
feet=lightning_prisoners_boots,id=86319,gems=160int,enchant=170haste
finger1=seal_of_the_profane,id=86211,reforge=spi_haste
finger2=watersoul_signet,id=86315,reforge=spi_mastery
trinket1=essence_of_terror,id=86388
