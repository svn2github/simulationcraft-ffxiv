#!./simc 

deathknight="Death_Knight_Frost_2h_T14N"
level=90
race=tauren
role=attack
position=back
talents=http://us.battle.net/wow/en/tool/talent-calculator#dZ!1...1.
spec=frost

# This default action priority list is automatically created based on your character.
# It is a attempt to provide you with a action list that is both simple and practicable,
# while resulting in a meaningful and good simulation. It may not result in the absolutely highest possible dps.
# Feel free to edit, adapt and improve it to your own needs.
# SimulationCraft is always looking for updates and improvements to the default action lists.

# Executed before combat begins. Accepts non-harmful actions only.

actions.precombat=flask,type=winters_bite
actions.precombat+=/food,type=black_pepper_ribs_and_shrimp
actions.precombat+=/frost_presence
actions.precombat+=/horn_of_winter
actions.precombat+=/snapshot_stats
actions.precombat+=/army_of_the_dead
actions.precombat+=/mogu_power_potion
actions.precombat+=/pillar_of_frost
actions.precombat+=/raise_dead

# Executed every time the actor is available.

actions=auto_attack
actions+=/pillar_of_frost
actions+=/mogu_power_potion,if=target.time_to_die<=30|(target.time_to_die<=60&buff.pillar_of_frost.up)
actions+=/raise_dead
actions+=/run_action_list,name=aoe,if=active_enemies>=5
actions+=/run_action_list,name=st,if=active_enemies<5

actions.aoe=unholy_blight,if=talent.unholy_blight.enabled
actions.aoe+=/pestilence,if=dot.blood_plague.ticking&talent.plague_leech.enabled,line_cd=28
actions.aoe+=/pestilence,if=dot.blood_plague.ticking&talent.unholy_blight.enabled&cooldown.unholy_blight.remains<49,line_cd=28
actions.aoe+=/howling_blast
actions.aoe+=/blood_tap,if=talent.blood_tap.enabled&buff.blood_charge.stack>10
actions.aoe+=/frost_strike,if=runic_power>76
actions.aoe+=/death_and_decay,if=unholy=1
actions.aoe+=/plague_strike,if=unholy=2
actions.aoe+=/blood_tap,if=talent.blood_tap.enabled
actions.aoe+=/frost_strike
actions.aoe+=/horn_of_winter
actions.aoe+=/plague_leech,if=talent.plague_leech.enabled&unholy=1
actions.aoe+=/plague_strike,if=unholy=1
actions.aoe+=/empower_rune_weapon

actions.st=plague_leech,if=talent.plague_leech.enabled&(dot.blood_plague.remains<1|dot.frost_fever.remains<1)
actions.st+=/outbreak,if=!dot.frost_fever.ticking|!dot.blood_plague.ticking
actions.st+=/unholy_blight,if=talent.unholy_blight.enabled&(!dot.frost_fever.ticking|!dot.blood_plague.ticking)
actions.st+=/soul_reaper,if=target.health.pct-3*(target.health.pct%target.time_to_die)<=35
actions.st+=/blood_tap,if=talent.blood_tap.enabled&(target.health.pct-3*(target.health.pct%target.time_to_die)<=35&cooldown.soul_reaper.remains=0)
actions.st+=/howling_blast,if=!dot.frost_fever.ticking
actions.st+=/plague_strike,if=!dot.blood_plague.ticking
actions.st+=/howling_blast,if=buff.rime.react
actions.st+=/obliterate,if=buff.killing_machine.react
actions.st+=/blood_tap,if=talent.blood_tap.enabled&buff.killing_machine.react
actions.st+=/blood_tap,if=talent.blood_tap.enabled&buff.blood_charge.stack>10&runic_power>76
actions.st+=/frost_strike,if=runic_power>76
actions.st+=/obliterate,if=blood=2|frost=2|unholy=2
actions.st+=/plague_leech,if=talent.plague_leech.enabled&(dot.blood_plague.remains<3|dot.frost_fever.remains<3)
actions.st+=/outbreak,if=dot.frost_fever.remains<3|dot.blood_plague.remains<3
actions.st+=/unholy_blight,if=talent.unholy_blight.enabled&(dot.frost_fever.remains<3|dot.blood_plague.remains<3)
actions.st+=/frost_strike,if=talent.runic_empowerment.enabled&frost=0
actions.st+=/frost_strike,if=talent.blood_tap.enabled&buff.blood_charge.stack<=10
actions.st+=/horn_of_winter
actions.st+=/frost_strike,if=talent.runic_corruption.enabled&buff.runic_corruption.down
actions.st+=/obliterate
actions.st+=/empower_rune_weapon,if=target.time_to_die<=60&(buff.mogu_power_potion.up|buff.golemblood_potion.up)
actions.st+=/blood_tap,if=talent.blood_tap.enabled&buff.blood_charge.stack>10&runic_power>=20
actions.st+=/frost_strike
actions.st+=/plague_leech,if=talent.plague_leech.enabled
actions.st+=/empower_rune_weapon

